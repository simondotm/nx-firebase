{
  "name": "simondotm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx run nx-firebase:build",
    "test": "nx run nx-firebase:test",
    "lint": "nx run nx-firebase:lint",
    "e2e": "nx run nx-firebase-e2e:e2e --silent=false",
    "release": "cd packages/nx-firebase && npm version",
    "release-help": "echo `npm run version -- v1.2.3` to set package version & commit tag",
    "compat:test": "npm run build && nx run compat:build && node dist/e2e/compat/main.js",
    "compat:clean": "nx run compat:build && node dist/e2e/compat/main.js --clean",
    "compat:setup": "npm run build && nx run compat:build && node dist/e2e/compat/main.js --setup"
  },
  "private": true,
  "devDependencies": {
    "@nx/devkit": "16.1.1",
    "@nx/eslint-plugin": "16.1.1",
    "@nx/jest": "16.1.1",
    "@nx/js": "16.1.1",
    "@nx/linter": "16.1.1",
    "@nx/node": "16.1.1",
    "@nx/plugin": "16.1.1",
    "@nx/workspace": "16.1.1",
    "@swc-node/register": "~1.4.2",
    "@swc/core": "~1.3.51",
    "@types/jest": "29.4.4",
    "@types/node": "18.7.1",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "28.1.3",
    "nx": "16.1.1",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.0.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@inquirer/prompts": "^2.3.0",
    "parse-help": "^1.0.0",
    "semver": "^7.3.8",
    "tslib": "^2.0.0",
    "yargs": "^17.7.2"
  }
}
